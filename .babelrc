{
  "presets": [
    "es2015",
    "stage-0",
    "next/babel",
    "react"
  ],
  "plugins": [
    [
      "styled-jsx/babel",      
      "module-resolver", {
        "root": ["."],
        "alias": {
          "styles": "./src/styles"
        },
        "cwd": "babelrc"
    }],
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ],
    ["react-intl", {
        "messagesDir": "./build/messages/"
    }]
  ]
}
