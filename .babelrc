{
  "presets": [
    "es2015",
    "stage-0",
    "next/babel",
    "react"
  ],
  "plugins": [
    [
      "styled-jsx/babel", { "plugins": ["styled-jsx-plugin-postcss"] }
    ],
    "markdown-in-js/babel",
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$", "md$"]
      }
    ],
    ["react-intl", {
        "messagesDir": "./build/messages/"
    }]
  ]
}
