{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-stage-0",
    ["next/babel", {
      "styled-jsx": {
        "plugins": [
          "styled-jsx-plugin-postcss"
        ]
      }
    }]
  ],
  "plugins": [
    "lodash",
    ["react-intl", {
      "messagesDir": "./build/messages/"
    }]
  ],
  "env": {
    "development" : {
      "compact": false
    }
  }
}
