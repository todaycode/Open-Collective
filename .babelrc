{
  "presets": [
    "es2015",
    "stage-0",
    ["next/babel", {
      "styled-jsx": { "optimizeForSpeed": true }
    }],
    "react"
  ],
  "plugins": [
    "transform-css-import-to-string",
    [
      "styled-jsx/babel", { "plugins": ["styled-jsx-plugin-postcss"] }
    ],
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "styles": "./src/styles",
        "markdown": "./src/markdown"
      },
      "cwd": "babelrc"
    }],
    "markdown-in-js/babel",
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$", "md$"]
      }
    ],
    ["react-intl", {
        "messagesDir": "./build/messages/"
    }]
  ]
}
